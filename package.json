{
  "name": "fe-interface",
  "version": "1.0.0",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "scripts": {
    "postinstall": "npm run theme",
    "dev": "next dev -p 3008",
    "build": "npm run build:prod",
    "build:dev": "npm run clean && cross-env EVN_FILE_NAME=dev next build",
    "build:prod": "npm run clean && cross-env EVN_FILE_NAME=prod next build",
    "export": "npm run export:prod",
    "export:dev": "cross-env EVN_FILE_NAME=dev next export",
    "export:prod": "cross-env EVN_FILE_NAME=prod next export",
    "start": "cross-env EVN_FILE_NAME=dev next start -p ${PORT:=3000}",
    "start:prod": "cross-env EVN_FILE_NAME=prod next start -p ${PORT:=3000}",
    "deploy:dev": "npm run build:dev && npm run export:dev && zip -r out.zip out/*",
    "deploy": "npm run build && npm run export && zip -r out-prod.zip out/*",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,md,json}'",
    "lint": "next lint",
    "clean": "rimraf .next out out.zip out-prod.zip",
    "theme": "chakra-cli tokens src/theme/index.ts",
    "theme:watch": "chakra-cli tokens src/theme/index.ts --watch",
    "i18n:extract": "NODE_ENV=development lingui extract --clean --overwrite --locale en",
    "i18n:extract:zh": "NODE_ENV=development lingui extract --clean --overwrite --locale zh",
    "i18n:compile": "lingui compile",
    "i18n": "npm run i18n:extract && npm run i18n:extract:zh && npm run i18n:compile"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@chakra-ui/cli": "2.4.1",
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@lingui/core": "^4.2.1",
    "@lingui/react": "^4.2.1",
    "@particle-network/auth": "^0.13.1",
    "@particle-network/provider": "^0.13.1",
    "@pqina/flip": "1.7.7",
    "@preconstruct/next": "^4.0.0",
    "@react-three/fiber": "^8.13.6",
    "ahooks": "3.7.2",
    "axios": "^1.2.0",
    "bignumber.js": "^9.1.0",
    "cross-env": "^7.0.3",
    "dayjs": "1.11.6",
    "ethers": "5.7.2",
    "formik": "^2.4.3",
    "framer-motion": "^10.12.16",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "next": "^13.4.5",
    "next-seo": "5.14.1",
    "pubsub-js": "^1.9.4",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-linkify-it": "^1.0.7",
    "react-particles": "^2.12.2",
    "react-toastify": "^9.1.1",
    "swiper": "^10.1.0",
    "three": "^0.155.0",
    "tsparticles-slim": "^2.12.0",
    "typed.js": "^2.0.16",
    "wagmi": "0.12.15",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-unicode-regex": "^7.18.6",
    "@lingui/cli": "^4.2.1",
    "@lingui/loader": "^4.2.1",
    "@lingui/macro": "^4.2.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/three": "^0.155.0",
    "autoprefixer": "^10.4.7",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.5",
    "next-transpile-modules": "^10.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "ios>=10",
      "chrome>=49",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  }
}
